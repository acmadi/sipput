            NavigationStyle.CssClass="btn-group btn-group-justified"
            OnCompleteButtonClick="addNewSIPICompleted" 
            OnNextButtonClick="processNextButton">
            <prop:StartNavigationTemplate>                    
                <com:TLinkButton Text="Next" CommandName="NextStep" ValidationGroup="addSIPI" CssClass="btn btn-default" />                    
            </prop:StartNavigationTemplate>
            <prop:StepNavigationTemplate>                
                <com:TLinkButton CausesValidation="False" CommandName="PreviousStep" Text="Previous" CssClass="btn btn-default" />               
                <com:TLinkButton Text="Next" CommandName="NextStep" ValidationGroup="addSIPI" CssClass="btn btn-default" />                                
            </prop:StepNavigationTemplate>
            <prop:FinishNavigationTemplate>
                <com:TLinkButton Text="Complete" CommandName="Complete" ValidationGroup="addSIPI" CssClass="btn btn-default" />                
            </prop:FinishNavigationTemplate>
            <com:TWizardStep ID="tahap1" Title="DATA SURAT PERMOHONAN" AllowReturn="true"> 
                <div class="panel-heading-process">
                    <h3 class='panel-title'>
                        <i class='fa fa-plus'></i>
                        TAMBAH SIPI (SURAT IZIN PENANGKAPAN IKAN) - DATA SURAT PERMOHONAN
                    </h3>
                </div>
                <div class="panel-body">
                    <p class="text-primary">
                        Proses ini di lakukan di UPT dimana Pemohon mengajukan permohonan pembuatan Izin Usaha Perikanan (IUP).
                    </p>
                    <blockquote>
                        Download Form Isian Standar Permohonan Baru Model A.1 dan Model A.2; serahkan kepada Pemohon untuk di isi, dan kemudian Pemohon menyerahkan Kembali ke petugas di UPT tempat dimana Pemohon mengajukan permohonan dengan melengkapi persyaratan permohonan sebagaimana tertera pada form Model A.1 dan Model A.2
                        <small>         
                            Download: 
                            <cite title="Model A1"><a href="#">Model A.1</a> | <a href="#">Model A.2</a></cite>
                        </small>
                    </blockquote>             
                    <div class="form-group" id="divCmbAddPemohon">
                        <label>Pilih Pemohon :</label>
                        <div class="radio" style="padding-left:0px">
                            <com:TDropDownList ID="cmbAddPemohon" AutoPostBack="false" CssClass="form-control chosen" Width="350px;" />                                                                                                        
                            <com:TRequiredFieldValidator ID="FieldAddPemohonValidator" InitialValue="none" ControlToValidate="cmbAddPemohon" Display="Dynamic" ErrorMessage="Pilih Pemohon" ValidationGroup="addSIPI" FocusOnError="true" CssClass="has-error help-block">				
                                <prop:ClientSide.OnValidationError>
                                    $('divCmbAddPemohon').addClassName('has-error');
                                    jQuery('#<%=$this->FieldAddPemohonValidator->ClientID%>').removeAttr('style');
                                </prop:ClientSide.OnValidationError>
                                <prop:ClientSide.OnValidationSuccess>
                                    $('divCmbAddPemohon').removeClassName('has-error');
                                </prop:ClientSide.OnValidationSuccess>
                            </com:TRequiredFieldValidator>
                        </div>
                    </div>
                    <div class="form-group" id="divTxtAddNoSurat">
                        <label>Masukan No. Surat Permohononan Model A.1 yang Diajukan Oleh Pemohon :</label>
                        <com:TTextBox ID="txtAddNomorSurat" CssClass="form-control" Width="150px" />			                            
                        <com:TRequiredFieldValidator ID="FieldAddNomorSuratValidator" ControlToValidate="txtAddNomorSurat" Display="Dynamic" ErrorMessage="Isi Kode Pemohon" ValidationGroup="addSIPI" FocusOnError="true" CssClass="has-error help-block">			
                            <prop:ClientSide.OnValidationError>
                                $('divTxtAddNoSurat').addClassName('has-error');
                                jQuery('#<%=$this->FieldAddNomorSuratValidator->ClientID%>').removeAttr('style');
                            </prop:ClientSide.OnValidationError>
                            <prop:ClientSide.OnValidationSuccess>
                                $('divTxtAddNoSurat').removeClassName('has-error');
                            </prop:ClientSide.OnValidationSuccess>
                        </com:TRequiredFieldValidator>
                    </div>
                    <div class="form-group">
                        <label>Pilih Tanggal Surat Permohonan Model A.1 yang Diajukan Oleh Pemohon :</label>
                        <div class="input-group">                            
                            <com:TCustomDatePicker ID="cmbAddTanggalSurat" DateFormat="dd-MMM-yyyy" Culture="id" FromYear="2014" UpToYear="<%=date('Y')%>" InputMode="DropDownList"/>
                        </div>                        
                    </div>              
                    <div class="form-group" id="divCmbAddJenisBidangUsaha">
                        <label>Pilih Jenis Bidang Usaha yang Dikehendaki :</label>
                        <div class="radio" style="padding-left:0px">
                            <com:TDropDownList ID="cmbAddJenisBidangUsaha" AutoPostBack="false" CssClass="form-control chosen-with-diselect" Width="350px;" />                                                                                                        
                            <com:TRequiredFieldValidator ID="FieldAddJenisBidangUsahaValidator" InitialValue="none" ControlToValidate="cmbAddJenisBidangUsaha" Display="Dynamic" ErrorMessage="Pilih Jenis Bidang Usaha" ValidationGroup="addSIPI" FocusOnError="true" CssClass="has-error help-block">				
                                <prop:ClientSide.OnValidationError>
                                    $('divCmbAddJenisBidangUsaha').addClassName('has-error');
                                    jQuery('#<%=$this->FieldAddJenisBidangUsahaValidator->ClientID%>').removeAttr('style');
                                </prop:ClientSide.OnValidationError>
                                <prop:ClientSide.OnValidationSuccess>
                                    $('divCmbAddJenisBidangUsaha').removeClassName('has-error');
                                </prop:ClientSide.OnValidationSuccess>
                            </com:TRequiredFieldValidator>
                        </div>
                    </div>
                    <div class="form-group" id="divAddModalUsaha">
                        <label>Sebutkan Banyaknya Modal Usaha :</label>
                        <com:TTextBox ID="txtAddModalUsaha" Attributes.OnKeyUp="formatangka(this,false)" CssClass="form-control" Width="200px" Attributes.PlaceHolder="Modal Usaha" />			                            
                        <com:TRequiredFieldValidator ID="FieldAddModalUsahaValidator" ControlToValidate="txtAddModalUsaha" Display="Dynamic" ErrorMessage="Isi Modal Usaha" ValidationGroup="addSIPI" FocusOnError="true" CssClass="has-error help-block">			
                            <prop:ClientSide.OnValidationError>
                                $('divAddModalUsaha').addClassName('has-error');
                                jQuery('#<%=$this->FieldAddModalUsahaValidator->ClientID%>').removeAttr('style');
                            </prop:ClientSide.OnValidationError>
                            <prop:ClientSide.OnValidationSuccess>
                                $('divAddModalUsaha').removeClassName('has-error');
                            </prop:ClientSide.OnValidationSuccess>
                        </com:TRequiredFieldValidator>                                            
                    </div>
                </div>
            </com:TWizardStep>
            <com:TWizardStep ID="tahap2" Title="BIODATA PEMOHON" AllowReturn="true"> 
                <div class="panel-heading-process">
                    <h3 class='panel-title'>
                        <i class='fa fa-plus'></i>
                        TAMBAH SIPI (SURAT IZIN PENANGKAPAN IKAN) - BIODATA PEMOHON
                    </h3>
                </div>
                <div class="panel-body">
                    <div class="form-group">
                        <label>Status Pemohon</label>
                        <div class="radio">
                            <label>
                                <%=$this->Pemohon->DataPemohon['Status']=='perorangan'?'PEORANGAN':'PERUSAHAAN'%>
