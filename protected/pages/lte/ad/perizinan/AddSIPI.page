                        </div> 
                    </div>     
                    <div class="form-group" id="divNPWPPemohon">
                        <label>No. NPWP Pemohon</label>
                        <div class="radio">
                            <label>
                                <%=$this->Pemohon->DataPemohon['NpwpPem']%>
                            </label>
                        </div> 
                    </div>
                </div>                
            </com:TWizardStep>
            <com:TWizardStep ID="tahap3" Title="Isi Formulir Model A.2 - Pilih Perusahaan" AllowReturn="true">
                <div class="panel-heading-process">
                    <h3 class='panel-title'>
                        <i class='fa fa-plus'></i>
                        TAMBAH SIPI (SURAT IZIN PENANGKAPAN IKAN) - PERUSAHAAN PEMOHON
                    </h3>
                </div>
                <div class="panel-body">
                    <div class="form-group" ids="divPilihPerusahaan">
                        <com:TActiveHiddenField ID="hiddenidperusahaan" />
                        <label>Daftar Perusahaan</label>
                        <com:TActiveDropDownList ID="cmbAddDaftarPerusahaan" OnSelectedIndexChanged="changePerusahaanPemohon" CssClass="form-control" Width="450px">
                            <prop:ClientSide.OnPreDispatch>
                                $('loading').show();                                             
                                $('<%=$this->cmbAddDaftarPerusahaan->ClientId%>').disabled='disabled';						
                            </prop:ClientSide.OnPreDispatch>
                            <prop:ClientSide.OnLoading>
                                $('<%=$this->cmbAddDaftarPerusahaan->ClientId%>').disabled='disabled';						
                            </prop:ClientSide.OnLoading>
                            <prop:ClientSide.OnComplete>																	
                                $('<%=$this->cmbAddDaftarPerusahaan->ClientId%>').disabled='';						                                            
                                $('loading').hide(); 
                            </prop:ClientSide.OnComplete>
                        </com:TActiveDropDownList>
                        <com:TRequiredFieldValidator ID="FieldAddPerusahaanValidator" ControlToValidate="hiddenidperusahaan" Display="Dynamic" ErrorMessage="Pilih Perusahaan" ValidationGroup="addSIPI" FocusOnError="true" CssClass="has-error help-block" />				                                                    
                    </div>
                    <div class="form-group">
                        <label>Status Perusahaan</label>
                        <div class="radio">
                            <label>
                                <com:TActiveLabel ID="lblStatusPerusahaan" Text="-" />
                            </label>
                        </div>                        
                    </div>     
                    <div class="form-group" id="divNamaPerusahaan">
                        <label>Nama Perusahaan</label>
                        <div class="radio">
                            <label>
                                <com:TActiveLabel ID="lblNamaPerusahaan" Text="-" />
                            </label>
                        </div>  
                    </div>
                    <div class="form-group" id="divNoAktePerusahaan">
                        <label>Nomor Akte Pendirian Perusahaan</label>
                        <div class="radio">
                            <label>
                                <com:TActiveLabel ID="lblNoAktePerusahaan" Text="-" />
                            </label>
                        </div>  
                    </div>
                    <div class="form-group">
                        <label>Tanggal Akte Pendirian Perusahaan</label>
                        <div class="radio">
                            <label>
